{
    "name": "devtarekmia/spell-money",
    "description": "By using this standalone package, you can convert numeric values into words in English based on Bangladeshi numeric counting (lakh-crore).",
    "keywords": [
        "tarekmia",
        "number to word",
        "lakh crore",
        "spell-money",
        "taka paisa",
        "number to indian english",
        "number to taka",
        "number to words",
        "spell money",
        "number bangladeshi words",
        "convert number to word"
    ],
    "homepage": "https://github.com/devtarekmia/spell-money",
    "license": "MIT",
    "authors": [
        {
            "name": "Tarek Mia",
            "email": "dev.tarekmia@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3.5"
    },
    "autoload": {
        "psr-4": {
            "TarekMia\\SpellMoney\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TarekMia\\SpellMoney\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {}
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}